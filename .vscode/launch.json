{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Run",
      "type": "python",
      "request": "launch",
      "program": "src/main.py",
      "cwd": "${workspaceFolder}/atari",
      "console": "integratedTerminal",
      "justMyCode": false,
      "autoReload": {
        "enable": false,
        "include": ["**/*.py", "**/*.pyw"]
      },
      "presentation": {
        "hidden": false,
        "group": "",
        "order": -99
      },
      "env": {
        "PYTORCH_MPS_HIGH_WATERMARK_RATIO": "0.0",
        "PYTORCH_MPS_LOW_WATERMARK_RATIO": "1"
      }
    }
  ]
}
